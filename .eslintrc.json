{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "commonjs": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "settings": {
        "react": {
          "version": "detect"
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "camelcase" : "off",
        "no-useless-constructor": "error",
        "no-unused-vars": "warn",
        "callback-return": ["warn", [ "callback", "next" ]]
    }
}
